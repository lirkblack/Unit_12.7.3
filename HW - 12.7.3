per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}

money = int(input("Введите сумму, которую хотите положить на счёт: "))

deposit = []

for value in per_cent:
    deposit.append(per_cent[value] * money / 100)
i = max(deposit)
print("Столько вы можете заработать в каждом банке: " + str(deposit))
print("Максимальная сумма, которую вы можете заработать: " + str(i) + " рублей.")
